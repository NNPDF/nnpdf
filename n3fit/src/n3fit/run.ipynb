    "lhapdf_path = \"/home/daksh/anaconda3/envs/environment_nnpdf/share/LHAPDF\"\n", # path to lhapdf folder
    "\n",
    "for runcard in range(total_runcards):\n",
    "    runcard_name = f\"Basic_runcard_{runcards[runcard]}\"\n",       
    "    for path in [lhapdf_path, n3fit_path, nnpdf_results_path]:\n",
    "        with catch_and_print(f\"{runcard_name} is not pre-existing in {path}\", comments=False):\n",
    "            print(f\"Removing pre-existing {runcard_name} from {path}\")\n",
    "            results = subprocess.run([\"rm\", \"-r\", f\"{path}/{runcard_name}\"], check=True)"
   ]
